<package name='MultiTypeSpike' version='0.0.1'>
    <depends on='BEAST.base' atleast='2.7.5'/>
    <depends on='SA' atleast='2.1.1'/>
    <depends on='BDMM-Prime' atleast='2.2.2'/>

    <service type="beast.base.core.BEASTInterface">
        <provider classname="multitypespike.clockmodel.PunctuatedClockModel"/>
        <provider classname="multitypespike.distribution.BranchSpikePrior"/>
        <provider classname="multitypespike.logger.SpikeLogger"/>
        <provider classname="multitypespike.logger.ExpectedHiddenEventsLogger"/>
        <provider classname="multitypespike.logger.HiddenEventsLogger"/>
        <provider classname="multitypespike.logger.SaltativeProportionLogger"/>
        <provider classname="multitypespike.operator.SpikeFlipOperator"/>
        <provider classname="multitypespike.operator.SpikeUpDownOperator"/>

    </service>

</package>
